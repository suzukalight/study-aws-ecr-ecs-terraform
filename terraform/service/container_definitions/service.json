[
  {
    "name": "sample-webapp",
    "image": "747695887408.dkr.ecr.ap-northeast-1.amazonaws.com/sample:latest",
    "essential": true,
    "portMappings": [
      {
        "protocol": "tcp",
        "hostPort": 23456,
        "containerPort": 23456
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "service",
        "awslogs-group": "sample-service"
      }
    },
    "secrets": [
      {
        "name": "DB_HOSTNAME",
        "valueFrom": "/sample/DB_HOSTNAME"
      },
      {
        "name": "DB_DATABASE",
        "valueFrom": "/sample/DB_DATABASE"
      },
      {
        "name": "DB_USERNAME",
        "valueFrom": "/sample/DB_USERNAME"
      },
      {
        "name": "DB_PASSWORD",
        "valueFrom": "/sample/DB_PASSWORD"
      }
    ]
  }
]
